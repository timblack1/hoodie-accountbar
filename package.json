    "vulcanize": ">= 1.4.2",
    "web-component-tester": "^3.3.9"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "files": [
    "demo/",
    "test/"
  ],
  "scripts": {
    "test-old": "gulp run_tests",
    "test": "node_modules/hoodie-server/bin/start --custom-ports=3002,3003,3004 & pid=$! && wct && kill $pid",
    "start": "gulp serve",
    "start-old": "node node_modules/hoodie-server/bin/start",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post && gp.sh timblack1 hoodie-accountbar"
  },
  "hoodie": {
    "plugins": [
      "hoodie-plugin-appconfig",
      "hoodie-plugin-users"
    ],
    "hoodiejs": "node_modules/hoodie/src/hoodie.js"
  }
