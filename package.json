    "jshint-stylish": "^2.0.0",
    "merge-stream": "^1.0.0",
    "opn": "^3.0.2",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.0.2",
    "semantic-release": "^4.0.2",
